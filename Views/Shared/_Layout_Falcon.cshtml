			<script>
				var navbarPosition = localStorage.getItem('phoenixNavbarPosition');
				var navbarVertical = document.querySelector('.navbar-vertical');
				var navbarTopVertical = document.querySelector('.content .navbar-top');
				var navbarTop = document.querySelector('[data-layout] .navbar-top:not([data-double-top-nav');
				var navbarDoubleTop = document.querySelector('[data-double-top-nav]');
				var navbarTopCombo = document.querySelector('.content [data-navbar-top="combo"]');

				if (localStorage.getItem('phoenixIsNavbarVerticalCollapsed') === 'true') {
					document.documentElement.className = 'navbar-vertical-collapsed';
				}

				if (navbarPosition === 'double-top') {
					topNavSlim = document.querySelector('[data-double-top-nav] .navbar-top-slim');
					topNavSlim.remove();
					bottomNav = document.querySelector('[data-double-top-nav] .navbar-top');
					bottomNav.removeAttribute('style');
					navbarDoubleTop.removeAttribute('style');
					navbarDoubleTop.classList.add('navbar-vertical');

				} else if (navbarPosition === 'top') {
					navbarTop.removeAttribute('style');
					navbarVertical.remove();
					navbarTopVertical.remove();
				} else if (navbarPosition === 'combo') {
					navbarVertical.removeAttribute('style');
					navbarTopCombo.removeAttribute('style');
					navbarTop.remove();
					navbarTopVertical.remove();
				} else {
					navbarVertical.removeAttribute('style');
					navbarTopVertical.removeAttribute('style');
					navbarTop.remove();
					navbarDoubleTop.remove();
				}
			</script>
